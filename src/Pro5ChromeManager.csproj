
<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <RootNamespace>Pro5ChromeManager</RootNamespace>
    <AssemblyName>Pro5ChromeManager</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <!-- This package is potentially used for JSON operations, keeping it for safety -->
